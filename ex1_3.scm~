(define (squares x)
  (* x x))
(define (sum-squares a b)
  (+ (squares a) (squares b)))
(define (sum-larger-squares a b c)
  (cond ((and (> a b) (> b c))(sum-squares a b))
	((and (> a b) (> c b))(sum-squares a c))
	((and (> b a) (> a c))(sum-squares b a))
	((and (> b a) (> c a))(sum-squares b c))))
(sum-larger-squares 3 4 5)